
create database inputFiles;

create table TRANSACTION(TRANSACTION_REF varchar(12)  primary key,VALUE_DATE date not null,PAYER_NAME varchar(35) not null,PAYER_ACCOUNT varchar(12) not null,PAYEE_NAME varchar(35) not null,PAYEE_ACCOUNT varchar(12) not null,AMOUNT double not null,STATUS varchar(25));


create table sanctionlist(ID int  primary key,NAME varchar(20));
